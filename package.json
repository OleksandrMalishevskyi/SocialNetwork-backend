{
  "name": "backend-socialnetwork",
  "version": "1.0.0",
  "description": "socialnetwork as facebook",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production pm2 start ./build/src/app.js -i 5 --attach --watch | bunyan",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | bunyan",
    "lint:check": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier:check": "prettier --check \"src/**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,json}\"",
    "build": "tsc -p .",
    "seeds:dev": "npx ts-node src/seeds.ts",
    "seeds:prod": "npx ts-node ./build/src/seeds.js"
  },
  "author": "Oleksandr Malishevskiy",
  "license": "ISC",
