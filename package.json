    "@sendgrid/mail": "^7.7.0",
    "@socket.io/redis-adapter": "^8.0.0",
    "@types/jest": "^29.4.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "bullmq": "^3.10.0",
    "bunyan": "^1.8.15",
    "canvas": "^2.11.0",
    "cloudinary": "^1.35.0",
